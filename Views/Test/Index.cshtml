@model IEnumerable<Klienci>

<h2>Klienci</h2>
<a href="@Url.Action("Create", "Test")" class="btn btn-primary">
    Dodaj klienta
</a> 

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Surname</th>
        <th>PESEL</th>
        <th>BirthYear</th>
        <th>Płeć</th>
    </tr>
    @foreach (var klient in Model) {
        <tr>
            <td>@klient.Id</td> 
            <td>@klient.Name</td>
            <td>@klient.Surname</td>
            <td>@klient.PESEL</td>
            <td>@klient.BirthYear</td>
            <td>@klient.Płeć</td>

            <td>
                <a  href="@Url.Action("Delete", "Test", new { id = klient.Id })">
                    Usuń
                </a>
            </td>
        </tr>
    }




</table>